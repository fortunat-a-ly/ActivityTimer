<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:id="@+id/nav_graph_results"
    app:startDestination="@id/DoneTasksList">

    <fragment
        android:id="@+id/DoneTasksList"
        android:name="com.fortunately.timepass.screens.doneTasks.DoneTaskList"
        android:label="Your results">

        <action
            android:id="@+id/action_TaskList_to_SubtaskList"
            app:destination="@id/SubtaskListFragment" >
        </action>
    </fragment>

    <fragment
        android:id="@+id/SubtaskListFragment"
        android:name="com.fortunately.timepass.screens.listScreens.DoneSubtaskListFragment">

        <argument
            android:name="taskId"
            app:argType="long"/>
    </fragment>

</navigation>